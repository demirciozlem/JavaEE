<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>TelefonRehberi</display-name>
  <servlet>
    <servlet-name>TelefonRehberServlet</servlet-name>
    <servlet-class>org.ismek.RehberIslem</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TelefonRehberServlet</servlet-name>
    <url-pattern>/RehberIslemMap</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InitExample</servlet-name>
    <servlet-class>org.ismek.InitParameterExample</servlet-class>
    <init-param>
      <param-name>parametre1</param-name>
      <param-value>55</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>InitExample</servlet-name>
    <url-pattern>/InitParamExample</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>TelefonRehberi.html</welcome-file>
  </welcome-file-list>
</web-app>